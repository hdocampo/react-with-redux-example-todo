{
    "extends": ["tslint:recommended", "tslint-react"],
    "rules": {
        "quotemark": false,
        "trailing-comma": false,
        "member-ordering": false,
        "semicolon": false,
        "member-access": false
    },
    "linterOptions": {
        "exclude": [
            "node_modules/**/*.ts"
        ]
    }
}